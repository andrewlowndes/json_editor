{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": ["DOM"],
    "jsx": "react",
    "outDir": "./build",
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}